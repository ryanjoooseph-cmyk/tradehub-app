{
  "version": 1,
  "automerge_label": "automerge",
  "branch_prefix": "agent",
  "queue": [
    { "id": "ui_shell_dashboard_001", "type": "frontend", "paths": ["app/dashboard/**"] },

    { "id": "auth_pages_001", "type": "frontend", "paths": ["app/login/**","app/logout/**","app/register/**","lib/auth/**"] },
    { "id": "layout_nav_001", "type": "frontend", "paths": ["app/layout.tsx","app/(components)/**","app/globals.css"] },

    { "id": "clients_backend_326", "type": "backend", "paths": ["app/api/clients/**","lib/db/**","lib/validation/**"] },
    { "id": "clients_frontend_321", "type": "frontend", "paths": ["app/clients/**","app/(components)/**"] },

    { "id": "jobs_backend_326", "type": "backend", "paths": ["app/api/jobs/**","lib/db/**","lib/validation/**"] },
    { "id": "jobs_frontend_321", "type": "frontend", "paths": ["app/jobs/**","app/(components)/**"] },

    { "id": "quotes_backend_326", "type": "backend", "paths": ["app/api/quotes/**","lib/db/**","lib/validation/**"] },
    { "id": "quotes_frontend_321", "type": "frontend", "paths": ["app/quotes/**","app/(components)/**"] },

    { "id": "payments_backend_326", "type": "backend", "paths": ["app/api/payments/**","lib/db/**"] },
    { "id": "payments_frontend_321", "type": "frontend", "paths": ["app/payments/**","app/(components)/**"] },

    { "id": "invoices_backend_326", "type": "backend", "paths": ["app/api/invoices/**","lib/db/**"] },
    { "id": "invoices_frontend_321", "type": "frontend", "paths": ["app/invoices/**","app/(components)/**"] },

    { "id": "tenants_backend_326", "type": "backend", "paths": ["app/api/tenants/**","lib/db/**"] },
    { "id": "tenants_frontend_321", "type": "frontend", "paths": ["app/tenants/**","app/(components)/**"] },

    { "id": "disputes_backend_326", "type": "backend", "paths": ["app/api/disputes/**","lib/db/**"] },
    { "id": "admin_disputes_frontend_321", "type": "frontend", "paths": ["app/admin/**","app/disputes/**","app/(components)/**"] }
  ],
  "conventions": {
    "pr_title_prefix": "Automated PR for",
    "plan_path_format": "agents/{task}/plan.md",
    "branch_name_format": "agent-{task}-{shortid}"
  }
}
